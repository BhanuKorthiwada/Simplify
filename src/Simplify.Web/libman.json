{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "twitter-bootstrap@4.5.0",
      "destination": "wwwroot/vendor/",
      "files": [
        "js/bootstrap.bundle.js",
        "js/bootstrap.bundle.min.js",
        "css/bootstrap.css",
        "css/bootstrap.min.css"
      ]
    },
    {
      "library": "jquery@3.5.1",
